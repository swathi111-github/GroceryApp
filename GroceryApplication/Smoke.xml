<suite name="GroceryApplication" parallel="test" thread-count="2">
<listeners>
		<listener class-name="listeners.ExtentReport"></listener>
		<listener class-name="listeners.RetryListener"></listener>
	</listeners>
	<test name="chromeTest">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="Smoke"></include>
			</run>
		</groups>
		<classes>
			<class name="testCases.LoginPageTest"></class>
			<class name="testCases.ManageContactTest"></class>
		</classes>
	</test>
</suite>